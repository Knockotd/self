<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="movie">
<!-- 아이템 테이블에서 itemid 와 itemname을 전부 가져오는 SQL -->
     <select id="allmovie"
     resultType="Movie">
     select id, name, description, img 
     from movie
     </select>
</mapper>